ST_MIN	./posix_time_watches.sv	/^  parameter ST_MIN = 0,$/;"	a
hour_o	./posix_time_watches.sv	/^  output logic [$clog2(23)-1:0] hour_o,$/;"	v
last_tact	./posix_time_watches.sv	/^logic                  last_tact;$/;"	v
last_tact_w	./posix_time_cnt.sv	/^logic                                last_tact_w;$/;"	v
min	./posix_time_watches.sv	/^logic [$clog2(59)-1:0] min;$/;"	v
min_o	./posix_time_watches.sv	/^  output logic [$clog2(59)-1:0] min_o,$/;"	v
posix_time_cnt	./posix_time_cnt.sv	/^module posix_time_cnt #($/;"	m
posix_time_watches	./posix_time_watches.sv	/^module posix_time_watches #($/;"	m
sec	./posix_time_watches.sv	/^logic [$clog2(59)-1:0] sec;$/;"	v
sec_blnk_o	./posix_time_watches.sv	/^  output logic                  sec_blnk_o,$/;"	v
sec_o	./posix_time_cnt.sv	/^  output logic [$clog2( 59 )-1:0] sec_o,$/;"	v
sec_o	./posix_time_watches.sv	/^  output logic [$clog2(59)-1:0] sec_o$/;"	v
tacts_cnt	./posix_time_cnt.sv	/^logic [$clog2( CLK_FREQ*(10**6) )-1:0] tacts_cnt;$/;"	v
