user_min	./posix_time_watches.sv	/^logic [$clog2(59)-1:0] user_min;$/;"	v
user_min_en	./posix_time_watches.sv	/^logic                  user_min_en;$/;"	v
user_time_en	./posix_time_watches.sv	/^logic                  user_time_en;$/;"	v
